{
	"manifest_version": 2,
	"name": "AspaklaryaLockdown",
	"version": "0.0",
	"url": "https://www.mediawiki.org/wiki/Extension:AspaklaryaLockdown",
	"author": "[https://www.hamichlol.org.il/משתמש:מקוה מקוה]",
	"descriptionmsg": "AspaklaryaLockdown-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.31.0"
	},
	"MessagesDirs": {
		"AspaklaryaLockdown": [
			"i18n"
		]
	},
	"AutoLoadClasses": {
		"MediaWiki\\Extension\\AspaklaryaLockDown\\ALDBData": "includes/ALDBData.php",
		"MediaWiki\\Extension\\AspaklaryaLockDown\\ALockDownForm": "includes/ALockDownForm.php",
		"MediaWiki\\Extension\\AspaklaryaLockDown\\ALockDownAction": "includes/ALockDownAction.php"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\AspaklaryaLockDown\\": "includes/"
	},
	"config": {
		"AspaklaryaLockdown": {
			"value": []
		}
	},
	"GroupPermissions": {
		"aspaklaryaEditor": {
			"aspaklarya-read-locked": true,
			"aspaklarya-edit-locked": true,
			"ALockDown": true
		},
		"sysop": {
			"aspaklarya-read-locked": true,
			"aspaklarya-edit-locked": true,
			"ALockDown": true
		}
	},
	"AvailableRights": [
		"aspaklarya-read-locked",
		"aspaklarya-edit-locked",
		"ALockDown"
	],
	"Hooks": {
		"LoadExtensionSchemaUpdates": "schema",
		"getUserPermissionsErrors": "main",
		"ApiCheckCanExecute": "main",
		"BeforeParserFetchTemplateRevisionRecord": "main"
	},
	"HookHandlers": {
		"schema": {
			"class": "MediaWiki\\Extension\\AspaklaryaLockDown\\SchemaUpdater"
		},
		"main": {
			"class": "MediaWiki\\Extension\\AspaklaryaLockDown\\AspaklaryaLockdown"
		}
	},
	"LogTypes": [
		"aspaklarya"
	],
	"LogNames": {
		"aspaklarya": "aspaklaryalockdownlog"
	},
	"LogHeaders": {
		"aspaklarya": "aspaklaryalockdownlogtext"
	},
	"LogActionsHandlers": {
		"aspaklarya/*": "LogFormatter"
	},
	"LogRestrictions": {
		"aspaklarya": "ALockDown"
	},
	"Actions": {
		"ALockDown": "MediaWiki\\Extension\\AspaklaryaLockDown\\ALockDownAction"
	}
}